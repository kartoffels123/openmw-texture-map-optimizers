{
  "n_format": "BC5/ATI2",
  "nh_format": "BC3/DXT5",
  "scale_factor": 1.0,
  "max_resolution": 2048,
  "min_resolution": 256,
  "invert_y": false,
  "reconstruct_z": true,
  "uniform_weighting": true,
  "use_dithering": false,
  "use_small_texture_override": true,
  "small_nh_threshold": 256,
  "small_n_threshold": 128,
  "resize_method": "CUBIC (Recommended - smooth surfaces + detail)",
  "preserve_compressed_format": true,
  "auto_fix_nh_to_n": true,
  "auto_optimize_n_alpha": true,
  "allow_compressed_passthrough": true,
  "enable_atlas_downscaling": false,
  "atlas_max_resolution": 4096
}